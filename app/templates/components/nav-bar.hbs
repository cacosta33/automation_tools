<nav class="navbar navbar-default">
	<div class="container-fluid">
		<div class="navbar-header">
			<a class="navbar-brand" href="#">{{title}}</a>
		</div>
		<ul class="nav navbar-nav navbar-right">
			{{#each menuItems as |item index|}}
				{{#unless item.restricted}}
					{{#unless item.links}}
						<li id="{{item.id}}-link"><a href="{{item.url}}">{{item.label}}</a></li>
					{{else}}
						<li class="dropdown">
							<a href="#" id="{{item.id}}-dropdown" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">{{item.label}}<span class="caret"></span></a>
							<ul class="dropdown-menu">
								{{#each item.links as |subItem index|}}
									<li id="{{subItem.id}}-link"><a href="{{subItem.url}}">{{subItem.label}}</a></li>
								{{/each}}
							</ul>
						</li>
					{{/unless}}
				{{/unless}}
			{{/each}}
		</ul>
	</div>
</nav>
